{
  "name": "brandy",
  "version": "0.2.0",
  "scripts": {
    "build:signed": "npm run pack && cross-env CSC_LINK=file://./cert.p12 CSC_KEY_PASSWORD=123456 build",
    "build": "npm run pack && build",
    "build:dir": "npm run pack && electron-builder --dir -o",
    "dev": "cross-env PORT=9080 node tasks/runner.js",
    "pack": "cross-env PORT=9080 npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --colors --config webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --colors --config webpack.renderer.config.js",
    "test": "npm run unit",
    "unit": "cross-env BABEL_ENV=testing-unit karma start test/unit/karma.conf.js",
    "postinstall": "cd app && npm install",
    "preversion": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "app/src/**/*.{js,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "build": {
    "asar": true,
    "extraMetadata": {
      "main": "./dist/main.js"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://",
        "channel": "latest"
      }
    ],
    "productName": "Brandy",
    "appId": "com.ph1p.brandy",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist"
    ],
    "mac": {
      "icon": "app/icons/icon.icns",
      "target": []
    }
  },
  "author": "Ph1p <phil@capshake.com>",
  "license": "MIT",
  "prettier": {
    "useTabs": false,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120,
    "trailingComma": "none"
  },
  "babel": {
    "comments": false,
    "env": {
      "testing-unit": {
        "presets": [
          "env"
        ],
        "plugins": [
          "istanbul",
          "transform-object-rest-spread"
        ]
      },
      "main": {
        "presets": [
          "env"
        ],
        "plugins": [
          "transform-object-rest-spread"
        ]
      },
      "renderer": {
        "presets": [
          "env",
          "vue"
        ],
        "plugins": [
          "transform-object-rest-spread"
        ]
      }
    },
    "plugins": [
      "transform-runtime"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.1",
    "babel-helpers": "^6.24.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-vue": "^0.2.0",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.23.0",
    "babili-webpack-plugin": "^0.1.2",
    "chai": "^3.5.0",
    "conventional-changelog-cli": "^2.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^1.0.0",
    "devtron": "^1.1.0",
    "electron": "^2.0.6",
    "electron-builder": "^20.27.0",
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.2",
    "electron-webpack": "^2.1.2",
    "eslint-plugin-html": "^2.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "feather-icons": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.3.1",
    "json-loader": "^0.5.7",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.1",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.13.1",
    "tree-kill": "^1.1.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "vue-hot-reload-api": "^2.0.7",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^15.2.6",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.4",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^2.4.0"
  }
}
